{
  "name": "kpi-lab",
  "version": "1.0.0",
  "description": "Стек технологій даного проекту:\r - Fastify \r - compiler Typescript",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./**/*.{ts,tsx,js}",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "pre-push": "lint-staged",
    "build": "tsc -w",
    "dev": "nodemon build/index.js",
    "start": "node build/index.js"
  },
  "husky": {
    "pre-push": "lint-staged",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,css,scss,md}": [
      "prettier --write",
      "npm run lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ireenna/node-kpi-lab.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ireenna/node-kpi-lab/issues"
  },
  "homepage": "https://github.com/ireenna/node-kpi-lab#readme",
  "dependencies": {
    "fastify": "^3.29.0",
    "fastify-plugin": "^3.0.1",
    "mongoose": "^6.3.5",
    "nodemon": "^2.0.16"
  },
  "devDependencies": {
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.39",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "prettier": "2.6.2",
    "typescript": "^4.7.3"
  }
}
